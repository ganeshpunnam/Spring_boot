package com.example.demo.persistance;



import java.util.List;

import com.example.demo.entity.Farnicher;

public interface FernicharDAOdeclartions {
	public List<Farnicher> displayAll(); //select all employees
	public Farnicher getById(int id); //select based on id--->jpa-get()
	public void save(Farnicher farnicher); //insert update  -->jpa-saveOrUpdate()
	public void deleteById(int id);

}
