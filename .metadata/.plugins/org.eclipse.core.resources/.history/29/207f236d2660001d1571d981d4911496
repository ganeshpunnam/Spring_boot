package com.example.demo.service;

public class Employee {
	private int id;
	private String name;
	private String location;
	private int salarey;
	public Employee(int id, String name, int salarey, String location) {
		super();
		this.id = id;
		this.name = name;
		this.location = location;
		this.salarey = salarey;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getLocation() {
		return location;
	}
	public void setLocation(String location) {
		this.location = location;
	}
	public int getSalarey() {
		return salarey;
	}
	public void setSalarey(int salarey) {
		this.salarey = salarey;
	}
	@Override
	public String toString() {
		return "Employee [id=" + id + ", name=" + name + ", location=" + location + ", salarey=" + salarey + "]";
	}
	

}
